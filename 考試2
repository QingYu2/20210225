class obj {
  int a, b;
    obj () {
      a = 10;
      b = 15;
 }
}

public class CallByRef {
  public static void main(String[] args) {
  obj obj = new obj();
  System.out.println;
  byRef(obj);
  System.out.println;
  }
  
  static void byRef(obj p) {
  int t;
  t = p.a;
  p.a = p.b;
  p.b = t;
  }
}  
